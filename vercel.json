{
    "builds": [
        {
            "src": "requirements.txt",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.13.5"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "royelhotel/wsgi.py"
        }
    ]
  }